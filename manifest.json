{
  "manifest_version": 2,
  "name": "Work Rota Creator",
  "version": "1.0",
  "description": "Create work rotas easily",
  "permissions": ["storage"],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["script.js"]
    }
  ]
}